<p id="notice"><%= notice %></p>

<h1>Trocas</h1>

<table>
  <thead>
    <tr>
      <th>Usuario doador</th>
      <th>Usuario receptor</th>
      <th>Material</th>
      <th>Raiting</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trocas.each do |troca| %>
      <tr>
        <td><%= troca.usuario_doador.nome %></td>
        <td><%= troca.usuario_receptor.nome %></td>
        <td><%= troca.material.titulo %></td>
        <td><%= troca.raiting %></td>
        <td><%= link_to 'Show', troca %></td>
        <td><%= link_to 'Edit', edit_troca_path(troca) %></td>
        <td><%= link_to 'Destroy', troca, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Troca', new_troca_path %>
